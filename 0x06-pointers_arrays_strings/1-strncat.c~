#include "holberton.h"

char *_strncat(char *dest, char *src, int n)
{
  int i, a;
  for (i = 0; dest[i] != '\0'; i++)
    {
    }
  /**for (i2 = 0; src[i2] != '\0'; i2++)
    {
    }*/
  for (a = 0; a != n; a++)
    {
      dest[i] = src[a];
      i++;
    }
  dest[i + 1] = '\0';
  return (dest);
}
